<p id="notice"><%= notice %></p>

<h1>Taxes</h1>

<table>
  <thead>
    <tr>
      <th>Shipping</th>
      <th>Costadd</th>
      <th>Ipi</th>
      <th>Icms</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taxes.each do |tax| %>
      <tr>
        <td><%= tax.shipping %></td>
        <td><%= tax.costAdd %></td>
        <td><%= tax.ipi %></td>
        <td><%= tax.icms %></td>
        <td><%= link_to 'Show', tax %></td>
        <td><%= link_to 'Edit', edit_tax_path(tax) %></td>
        <td><%= link_to 'Destroy', tax, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tax', new_tax_path %>
